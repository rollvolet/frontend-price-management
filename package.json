{
  "name": "frontend-price-management",
  "version": "1.4.1",
  "private": true,
  "description": "Frontend of the Rollvolet price management application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollvolet/frontend-price-management.git"
  },
  "license": "MIT",
  "author": "Erika Pauwels <erika.pauwels@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "npm run postcss:build && ember build --environment=production",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\"",
    "lint:css": "stylelint \"**/*.css\"",
    "lint:css:fix": "concurrently \"npm:lint:css -- --fix\"",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "postcss:build": "postcss ./app/styles/tailwind-input.css -o ./public/assets/tailwind.css",
    "postcss:watch": "postcss ./app/styles/tailwind-input.css -o ./public/assets/tailwind.css --watch --verbose",
    "release": "release-it",
    "start": "ember serve",
    "test": "concurrently \"npm:lint\" \"npm:test:*\" --names \"lint,test:\"",
    "test:ember": "ember test"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.5",
    "@babel/plugin-proposal-decorators": "^7.22.5",
    "@ember/optional-features": "^2.0.0",
    "@ember/render-modifiers": "^1.0.2",
    "@ember/string": "^3.1.1",
    "@ember/test-helpers": "^3.0.3",
    "@glimmer/component": "^1.1.2",
    "@glimmer/tracking": "^1.1.2",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/forms": "^0.5.6",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "broccoli-asset-rev": "^3.0.0",
    "concurrently": "^8.2.0",
    "date-fns": "^3.2.0",
    "ember-async-data": "^1.0.3",
    "ember-auto-import": "^2.6.3",
    "ember-cli": "~5.0.0",
    "ember-cli-app-version": "^6.0.0",
    "ember-cli-babel": "^7.26.11",
    "ember-cli-clean-css": "^2.0.0",
    "ember-cli-dependency-checker": "^3.3.1",
    "ember-cli-htmlbars": "^6.2.0",
    "ember-cli-inject-live-reload": "^2.1.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-terser": "^4.0.2",
    "ember-cli-update": "^1.0.1",
    "ember-click-outside": "^6.1.0",
    "ember-composable-helpers": "^5.0.0",
    "ember-concurrency": "^3.1.1",
    "ember-copy": "^2.0.1",
    "ember-css-transitions": "^2.1.0",
    "ember-data": "~5.0.0",
    "ember-dynamic-format-currency": "https://github.com/erikap/ember-dynamic-format-currency#master",
    "ember-fetch": "^8.1.2",
    "ember-file-upload": "^8.4.1",
    "ember-keyboard": "^8.2.1",
    "ember-load-initializers": "^2.1.2",
    "ember-math-helpers": "^4.0.0",
    "ember-modifier": "^4.1.0",
    "ember-mu-login": "^3.0.0",
    "ember-mu-transform-helpers": "^2.1.1",
    "ember-page-title": "^7.0.0",
    "ember-power-select": "^7.2.0",
    "ember-qunit": "^7.0.0",
    "ember-resolver": "^10.1.0",
    "ember-simple-auth": "^6.0.0",
    "ember-source": "~5.0.0",
    "ember-svg-jar": "^2.3.3",
    "ember-template-lint": "^5.10.1",
    "ember-tooltips": "^3.6.0",
    "ember-truth-helpers": "^4.0.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-ember": "^11.8.0",
    "eslint-plugin-n": "^16.0.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-qunit": "^7.3.4",
    "loader.js": "^4.7.0",
    "postcss": "^8.4.33",
    "postcss-cli": "^10.1.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.8",
    "qunit": "^2.19.4",
    "qunit-dom": "^2.0.0",
    "release-it": "^17.0.1",
    "remixicon": "^4.1.0",
    "stylelint": "^15.7.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-prettier": "^3.0.0",
    "tailwindcss": "^3.4.1",
    "torii": "https://github.com/jmonster/torii.git#bbbd0ba317da193039193e47295224d2290b8443",
    "torii-azure-ad2-provider": "^1.0.5",
    "tracked-built-ins": "^3.3.0",
    "webpack": "5.86.0"
  },
  "engines": {
    "node": "16.* || >= 18"
  },
  "ember": {
    "edition": "octane"
  }
}
