{
  "name": "frontend-price-management",
  "version": "0.0.0",
  "private": true,
  "description": "Frontend of the Rollvolet price management application",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rollvolet/frontend-price-management.git"
  },
  "license": "MIT",
  "author": "Erika Pauwels <erika.pauwels@gmail.com>",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build --environment=production",
    "lint": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:!(fix)\"",
    "lint:fix": "npm-run-all --print-name --aggregate-output --continue-on-error --parallel \"lint:*:fix\"",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "postcss:build": "postcss ./app/styles/tailwind-input.css -o ./public/assets/tailwind.css",
    "postcss:watch": "postcss ./app/styles/tailwind-input.css -o ./public/assets/tailwind.css --watch --verbose",
    "start": "ember serve",
    "test": "npm-run-all --print-name \"lint\" \"test:*\"",
    "test:ember": "ember test"
  },
